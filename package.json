    "@types/node": "^20.8.7",
    "@types/pg": "^8.10.7",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.51.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "git-commit-msg-linter": "^5.0.4",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "lint-staged": "^15.0.2",
    "npm-check-updates": "^16.14.6",
